{
    "name": "samwilson/twyne",
    "description": "A web-based journal for writing and photography.",
    "license": "GPL-3.0-or-later",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "addwiki/mediawiki-api": "^2.7",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "endroid/qr-code": "^4.2",
        "league/commonmark": "^1.6",
        "league/flysystem-aws-s3-v3": "^1.0",
        "longitude-one/doctrine-spatial": "^3.0",
        "samwilson/phpflickr": "^4.14",
        "sensio/framework-extra-bundle": "^6.0",
        "spomky-labs/otphp": "^10.0",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/process": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.4.*",
        "symfonycasts/reset-password-bundle": "^1.1",
        "twig/extra-bundle": "^2.12 || ^3.0",
        "twig/twig": "^2.12 || ^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.3",
        "ergebnis/composer-normalize": "^2.5",
        "mediawiki/minus-x": "^1.1",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/maker-bundle": "^1.21",
        "symfony/phpunit-bridge": "^5.4",
        "symfony/stopwatch": "^5.4",
        "symfony/web-profiler-bundle": "^5.4"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "./bin/console cache:clear"
        ],
        "post-update-cmd": [
            "./bin/console cache:clear"
        ],
        "docs": [
            "cd docs && make html SPHINXOPTS=\"-W\""
        ],
        "test": [
            "composer validate",
            "composer normalize --dry-run",
            "./bin/console lint:twig ./templates",
            "./bin/console lint:yaml ./config",
            "./bin/console lint:yaml ./translations",
            "phpcs",
            "minus-x check .",
            "./bin/phpunit"
        ]
    }
}
